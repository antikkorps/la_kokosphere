{
  "name": "la-kokosphere",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run studio\"",
    "studio": "cd studio-la_kokosphere && npm run dev",
    "build": "astro build",
    "build:studio": "cd studio-la_kokosphere && npm run build",
    "build:all": "npm run build && npm run build:studio && npm run copy:studio",
    "copy:studio": "mkdir -p dist/admin && cp -r studio-la_kokosphere/dist/* dist/admin/",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.1",
    "@astrojs/tailwind": "^6.0.2",
    "@sanity/astro": "^3.2.6",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "astro": "^5.11.0",
    "gsap": "^3.13.0",
    "sharp": "^0.34.2",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
